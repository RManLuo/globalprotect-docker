logoutput: stderr

internal: 0.0.0.0 port=1080
internal: :: port=1080
external: ##TUN0_IPV4##
external: ##TUN0_IPV6##

socksmethod: none
clientmethod: none

user.privileged: root
user.notprivileged: nobody

client pass {
    from: 0/0 port 1-65535 to: 0/0
    log: error connect
}

socks pass {
    from: 0/0 to: 0/0
    protocol: tcp udp
    command: bind connect udpassociate bindreply udpreply
    log: error connect
}